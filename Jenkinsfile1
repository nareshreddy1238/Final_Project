pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building'
                sh 'mvn clean package'
            }
        }
        stage('Deploy') {
            steps {
                sshagent(['Tomcat_user']) {
                   sh 'scp -i centos@3.95.192.72:~/jenkins_javaproject/target/WebAppCal-1.2.8.war ~/apache-tomcat-7.0.94/webapps/' 
    }       
   }
 }

}
}

    
